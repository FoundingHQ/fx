{
  "name": "@founding/template",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://founding.so",
  "bugs": "https://github.com/FoundingHQ/fx/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/FoundingHQ/fx",
    "directory": "packages/local"
  },
  "author": "@FoundingHQ",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "fx:add": "fx add",
    "fx:bootstrap": "fx bootstrap",
    "fx:list": "fx list",
    "fx:remove": "fx remove",
    "lint": "next lint",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:migrate:dev": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma migrate dev",
    "prisma:migrate:prod": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma migrate deploy",
    "prisma:reset": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma migrate reset",
    "prisma:seed": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma db seed",
    "start": "next start"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "next",
      "next/core-web-vitals"
    ]
  },
  "dependencies": {
    "@prisma/client": "^3.2.1",
    "babel-plugin-superjson-next": "^0.4.1",
    "cookie": "^0.4.1",
    "cross-fetch": "^3.1.4",
    "dotenv-cli": "^4.0.0",
    "next": "^11.1.2",
    "next-connect": "^0.11.0",
    "posthog-js": "^1.14.1",
    "prisma": "^3.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-query": "^3.26.0",
    "superjson": "^1.7.5"
  },
  "devDependencies": {
    "@founding/fx": "*",
    "@types/cookie": "^0.4.1",
    "@types/react": "^17.0.21",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
