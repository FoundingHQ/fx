{
  "name": "@founding/cli",
  "version": "0.0.0",
  "description": "Scaffold your application",
  "keywords": [
    "cli",
    "scaffold"
  ],
  "homepage": "https://github.com/FoundingHQ/scaffold",
  "bugs": "https://github.com/FoundingHQ/scaffold/issues",
  "repository": "FoundingHQ/scaffold",
  "author": "@FoundingHQ",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "fs": "./bin/run"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.3",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.9",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "chai": "^4.2.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "fs",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
