{
  "name": "@founding/template",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://founding.so",
  "bugs": "https://github.com/FoundingHQ/fx/issues",
  "repository": "https://github.com/FoundingHQ/fx",
  "author": "@FoundingHQ",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:migrate:dev": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma migrate dev",
    "prisma:migrate:prod": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma migrate deploy",
    "prisma:reset": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma migrate reset",
    "prisma:seed": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma db seed",
    "start": "next start"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "next",
      "next/core-web-vitals"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.4.1",
    "@prisma/client": "^3.2.1",
    "@stripe/stripe-js": "^1.17.1",
    "babel-plugin-superjson-next": "^0.4.1",
    "bcrypt": "^5.0.1",
    "cookie": "^0.4.1",
    "cross-fetch": "^3.1.4",
    "dotenv-cli": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "next": "^11.1.2",
    "next-connect": "^0.11.0",
    "nodemailer": "^6.6.3",
    "passport": "^0.5.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "posthog-js": "^1.14.1",
    "prisma": "^3.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-query": "^3.26.0",
    "stripe": "^8.176.0",
    "superjson": "^1.7.5",
    "tailwind-styled-components": "^2.0.3",
    "tailwindcss-classnames": "^2.2.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.3.3",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie": "^0.4.1",
    "@types/passport": "^1.0.7",
    "@types/passport-google-oauth20": "^2.0.10",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/react": "^17.0.21",
    "@types/uuid": "^8.3.1",
    "autoprefixer": "^10.3.4",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "postcss": "^8.3.6",
    "tailwindcss": "^2.2.15",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
