{
  "name": "@founding/fx",
  "version": "0.2.0",
  "keywords": [],
  "description": "CLI to add prebuilt features to Founding apps",
  "repository": {
    "type": "git",
    "url": "https://github.com/FoundingHQ/fx",
    "directory": "packages/fx"
  },
  "author": "@FoundingHQ",
  "license": "MIT",
  "bin": {
    "fx": "./dist/fx.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "cli": "node -r ts-node/register src/fx.ts",
    "dev": "ncc build ./src/fx.ts -w -o ./dist/",
    "prerelease": "rimraf ./dist/",
    "release": "ncc build ./src/fx.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "prepublish": "npm run release"
  },
  "devDependencies": {
    "@founding/devkit": "^0.2.0",
    "@types/async-retry": "^1.4.3",
    "@types/node": "^16.11.6",
    "@types/prompts": "^2.0.14",
    "@vercel/ncc": "^0.31.1",
    "async-retry": "^1.3.3",
    "chalk": "^4.1.2",
    "commander": "^8.3.0",
    "prompts": "^2.4.2"
  },
  "engines": {
    "node": ">=12.22.0"
  }
}
