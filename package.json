{
  "name": "fx",
  "private": true,
  "scripts": {
    "postinstall": "lerna bootstrap --force-local && link-parent-bin",
    "prepare": "husky install",
    "publish": "lerna publish --no-private",
    "clean": "lerna clean && lerna run clean",
    "dev": "lerna run dev --parallel",
    "test": "jest",
    "reset": "npm run clean && npm run reset:local && npm i",
    "reset:links": "lerna link --force-local",
    "reset:local": "ts-node ./scripts/local.ts && lerna link --force-local"
  },
  "dependencies": {
    "@types/jest": "^27.0.2",
    "@types/rimraf": "^3.0.2",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lerna": "^4.0.0",
    "link-parent-bin": "^2.0.0",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
